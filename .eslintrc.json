{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "airbnb/base",
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "indent": [2, 2],
    "linebreak-style": [2, "unix"],
    "quotes": [2, "single"],
    "semi": [2, "always"],
    "no-multi-spaces": [2, { "exceptions": { "VariableDeclarator": true, "ImportDeclaration": true } }],
    "key-spacing": [2, { "align": "colon" }],
    "no-param-reassign": [2, {"props": false}],
    "arrow-body-style": 0,
    "strict": 0
  },
  "globals": {
    "prettyPrint": true
  }
}
